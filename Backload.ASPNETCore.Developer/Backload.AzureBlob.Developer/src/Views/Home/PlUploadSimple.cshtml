@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" dir="ltr">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
    <title>Plupload - Simple Theme</title>
</head>

<body style="font: 13px Verdana; background: #eee; color: #333">
    <h1>Custom simple example</h1>
    <p>Shows you how to use the core plupload API.</p>

    <div id="filelist">Your browser doesn't have Flash, Silverlight or HTML5 support.</div>
    <br />
    <div id="container">
        <a id="pickfiles" href="javascript:;">[Select files]</a>
        <a id="uploadfiles" href="javascript:;">[Upload files]</a>
    </div>


    <!-- jQuery and Bootstrap script files -->
    <script src="~/Scripts/jquery-1.12.3.min.js"></script>

    <!-- PlUpload script -->
    <script src="~/Backload/Client/moxie/plupload/js/plupload.full.min.js"></script>


    <!-- The demo application script -->
    <script src="~/Scripts/demos.moxie.plupload.simple.js"></script>

</body>
</html>
